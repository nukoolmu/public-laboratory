filter(){

    sed 1d ${1}|while IFS=, read -r NO API_NAME PROTOCAL ORIGIN_1 ORIGIN_2 URI METHOD HEADER BODY
    do

        #this function curl and  logs
        #bypasscurl
            #curl_cmd "$METHOD" "$HEADER" "$BODY" "${ORIGIN_1}" "${URI}" "${PROTOCAL}" > $TMP_PATH/a.json 2>/dev/null
            #curl_cmd "$METHOD" "$HEADER" "$BODY" "${ORIGIN_2}" "${URI}" "${PROTOCAL}" > $TMP_PATH/b.json 2>/dev/null
        # sorting file

        . ${INC_PATH}/${API_NAME}

        # call function sort file
        ${API_NAME} "${SRC_PATH}/${API_NAME}_CLOUD.json" > ${TMP_PATH}/${API_NAME}_CLOUD.sort
        ${API_NAME} "${SRC_PATH}/${API_NAME}_PREM.json" > ${TMP_PATH}/${API_NAME}_PREM.sort

        jq_compare   "${TMP_PATH}/${API_NAME}_CLOUD.sort"  "${TMP_PATH}/${API_NAME}_PREM.sort"   2>/dev/null

    done
}