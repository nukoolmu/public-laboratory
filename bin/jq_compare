jq_compare(){

  FILE_A=${1}
  FILE_B=${2}

   if [ -z "$(cat ${TMP_PATH}/${FILE_A})" ] || [ -z "$(cat ${TMP_PATH}/${FILE_B})" ];then
     RESULT=false
   else
     RESULT=$(jq --argfile a ${TMP_PATH}/${FILE_A} --argfile b ${TMP_PATH}/${FILE_B} '($a | (.. | arrays) |= sort) as $a | ($b | (.. | arrays) |= sort) as $b | $a == $b') 
   fi
   
    if [ $? -ne 0 ];then
        RESULT=false
    fi

   report "${API_NAME}" "${RESULT}"
   ck_outdata "${RESULT}"
}