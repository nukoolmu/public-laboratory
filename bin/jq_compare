jq_compare(){

   if [ -z "$(cat ${TMP_PATH}/a.json)" ] || [ -z "$(cat ${TMP_PATH}/b.json)" ];then
     RESULT=false
   else
     RESULT=$(jq --argfile a ${TMP_PATH}/a.json --argfile b ${TMP_PATH}/b.json -n '($a | (.. | arrays) |= sort) as $a | ($b | (.. | arrays) |= sort) as $b | $a == $b') 
   fi
   
    if [ $? -ne 0 ];then
        RESULT=false
    fi

   report "${API_NAME}" "${RESULT}"
   ck_outdata "${RESULT}"
}