getPromotionsByCodes(){
    SRC_FILE=${1}

    if [ -f ${SRC_FILE} ];then

    jq '[.data[] | {id: .id, title: .title, titleEn: .titleEn, icon: .icon, sanitizedName: .sanitizedName,publish: .publish, priority: .priority, lastUpdated: .lastUpdated, tags: .tags, type: .type, conditionCode: .conditionCode, subShelves: .subShelves, items: .items}] | sort_by(.id)' ${SRC_FILE} 
    
    else
        echo "no src file exist"
    fi
}